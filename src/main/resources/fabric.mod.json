{
  "schemaVersion": 1,
  "id": "cesium",
  "version": "${version}",
  "name": "Cesium",
  "description": "An alternative storage format for Minecraft worlds which improves performance and reliability.",
  "authors": [
    "JellySquid",
    "Yamayaki"
  ],
  "contact": {
    "homepage": "https://github.com/yamayaki1",
    "sources": "https://github.com/yamayaki1/cesium-fabric"
  },
  "license": "LGPL-3.0",
  "icon": "assets/cesium/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.yamayaki.cesium.CesiumMod"
    ]
  },
  "accessWidener": "cesium.accesswidener",
  "mixins": [
    "cesium.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.10",
    "minecraft": [
      "1.20.6",
      "1.21-beta.1",
      "1.21-beta.2",
      "1.21-beta.3",
      "1.21-beta.4",
      "1.21-rc.1"
    ]
  },
  "breaks": {
    "c2me-rewrites-chunkio": "*",
    "c2me-rewrites-chunk-serializer": "*"
  }
}
